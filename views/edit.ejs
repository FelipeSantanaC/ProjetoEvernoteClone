<%- include("_header") -%>

<h1>Atualizando uma Nota</h1>

<form action="/<%= model.note_id %>/edit" method="post">
  <div class="form-horizontal">

    <%- include("_editor") -%>

    <div class="form-group row">
      <label class="col-form-label col-sm-2"></label>
      <div class="col-sm-10">
        <input type="submit" value="Atualizar" class="btn btn-default btn-warning" />
        <a class="btn btn-outline-dark cancel" href="/">Cancelar</a>
      </div>
    </div>
  </div>
</form>

<%- include("_footer") -%>